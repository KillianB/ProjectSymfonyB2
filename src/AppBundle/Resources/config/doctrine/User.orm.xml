<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="User">
    <id name="idUser" type="integer" column="id_User">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomUser" type="string" column="nom_User" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomUser" type="string" column="prenom_User" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="classeUser" type="string" column="classe_User" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="roleUser" type="string" column="role_User" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mailUser" type="string" column="mail_User" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telUser" type="integer" column="tel_User" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idSlackUser" type="string" column="id_slack_User" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="passwordUser" type="string" column="password_User" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="isReset" type="integer" column="isReset" length="1" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
